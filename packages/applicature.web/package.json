{
  "name": "@applicature/multivest.web",
  "version": "0.1.4",
  "private": false,
  "license": "UNLICENSED",
  "description": "Web Plugin",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "build": "gulp build",
    "lint": "tslint '{src/**/*.ts,test/**/*.ts}'",
    "precommit": "npm run lint ; npm run test:coverage ; npm run test:security",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=*/**/*.ts",
    "test:ci": "jest --coverage --testResultsProcessor ./node_modules/jest-junit --collectCoverageFrom=*/**/*.ts",
    "test:security": "nsp check"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:applicature/multivest.web.git"
  },
  "dependencies": {
    "@applicature/multivest.core": "^0.1.0",
    "compression": "^1.7.1",
    "config": "^1.29.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.9.0",
    "http-status": "^1.0.1",
    "joi": "^13.1.1",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "@types/agenda": "^1.0.1",
    "@types/compression": "0.0.35",
    "@types/config": "0.0.33",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.0",
    "@types/helmet": "0.0.37",
    "@types/jest": "^22.0.1",
    "@types/joi": "^13.0.5",
    "@types/method-override": "0.0.31",
    "@types/node": "^8.5.2",
    "@types/winston": "^2.3.7",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.3",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-junit": "^3.4.1",
    "nsp": "^3.1.0",
    "source-map-support": "^0.5.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "files": [
    "dist/",
    "src/",
    "index.d.ts",
    "index.js"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/.*?\\.d\\.ts$"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ]
  },
  "jest-junit": {
    "output": "./coverage/junit.xml"
  }
}
