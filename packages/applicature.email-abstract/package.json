{
  "name": "@applicature-private/applicature-sdk.email-abstract",
  "version": "2.0.0",
  "private": false,
  "license": "UNLICENSED",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "build": "gulp clean && gulp build",
    "lint": "tslint '{index.ts,src/**/*.ts,test/**/*.ts}'",
    "precommit": "npm run lint ; npm run test:ci",
    "test": "jest",
    "test:ci": "jest --coverage --forceExit",
    "test:sonar": "sonar-scanner -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$SONARCLOUD_TOKEN",
    "test:lint": "tslint --force --out ./test-reports/tslint.json -t json --project ./tsconfig.json '{index.ts,src/**/*.ts,test/**/*.ts}'"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:applicature/multivest.email.abstract.git"
  },
  "dependencies": {
    "@applicature-private/applicature-sdk.plugin-manager": "^2.0.0",
    "config": "^1.26.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "mongo-mock": "^3.0.0",
    "@types/config": "0.0.34",
    "@types/jest": "^23.3.3",
    "@types/joi": "^13.6.0",
    "@types/node": "^10.11.4",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-cli": "^20.0.4",
    "jest-junit": "^5.2.0",
    "jest-multi-test-result-processor": "^1.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.1.1",
    "ts-jest": "^23.10.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "files": [
    "dist/",
    "src/",
    "index.d.ts",
    "index.js"
  ],
  "jest-multi-test-result-processor": {
    "processors": [
      "jest-junit",
      "jest-sonar-reporter"
    ]
  },
  "jestSonar": {
    "reportPath": "./test-reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./test-reports/junit.xml"
  }
}
