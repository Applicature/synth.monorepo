{
  "name": "@applicature/multivest.core",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "description": "Plugin manager",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "lint": "esw *.js src config --color",
    "precommit": "npm run lint ; npm run test ; npm run test:security",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors ./tests/ --recursive",
    "test:watch": "npm test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors ./tests --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report-coverage": "nyc --reporter=lcov --reporter=text-lcov npm run test",
    "test:security": "nsp check"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:applicature/multivest.plugin-manager.git"
  },
  "dependencies": {
    "agenda": "^1.0.3",
    "bignumber.js": "^4.1.0",
    "config": "^1.26.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "@types/agenda": "^1.0.1",
    "@types/bignumber.js": "^4.0.3",
    "@types/config": "0.0.33",
    "@types/mongodb": "^2.2.18",
    "@types/node": "^8.5.2",
    "@types/winston": "^2.3.7",
    "typescript": "^2.6.2",
    "web3-typescript-typings": "^0.7.2"
  }
}
