{
  "name": "@applicature-private/multivest.core",
  "version": "2.0.1",
  "private": false,
  "license": "UNLICENSED",
  "description": "Plugin manager",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp clean && gulp build",
    "lint": "tslint '{src/**/*.ts,test/**/*.ts}'",
    "precommit": "npm run lint ; npm run test:coverage",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=*/**/*.ts",
    "test:ci": "jest --coverage --testResultsProcessor ./node_modules/jest-junit --collectCoverageFrom=*/**/*.ts",
    "test:sonar": "sonar-scanner -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$SYNTH_BACKEND_SONARCLOUD_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:applicature/multivest.plugin-manager.git"
  },
  "dependencies": {
    "agenda": "^1.0.3",
    "bignumber.js": "^6.0.0",
    "config": "^1.30.0",
    "handlebars": "^4.0.11",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/agenda": "^1.0.2",
    "@types/config": "0.0.33",
    "@types/handlebars": "^4.0.36",
    "@types/jest": "^22.2.2",
    "@types/mongodb": "^2.2.20",
    "@types/node": "^8.10.1",
    "@types/winston": "^2.3.8",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^3.2.4",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-junit": "^3.6.0",
    "sonarqube-scanner": "^2.1.0",
    "source-map-support": "^0.5.4",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "ts-jest": "^23.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/.*?\\.d\\.ts$",
      "<rootDir>/.*?\\.mock\\.ts$"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ]
  },
  "jest-junit": {
    "output": "./test-reports/junit.xml"
  }
}
