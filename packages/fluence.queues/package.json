{
  "name": "@applicature/queue-service",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-ts && npm run tslint",
    "watch-node": "nodemon dist/index.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "mocha -r ts-node/register test/**/*.test.ts --ui qunit --exit",
    "test:ci": "nyc npm run test -- --reporter mocha-junit-reporter --reporter-options mochaFile=./coverage/junit.xml ",
    "test:watch": "mocha -r ts-node/register test/**/*.test.ts --ui qunit --exit --watch --watch-extensions ts",
    "build-ts": "tsc  --declaration",
    "watch-ts": "tsc -w  --declaration",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "nsp": "nsp check ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.1.2",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.17.0",
    "nodemon": "^1.15.1",
    "nsp": "^3.1.0",
    "nyc": "^11.6.0",
    "shelljs": "^0.8.1",
    "source-map-support": "^0.5.4",
    "supertest": "^3.0.0",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "aws-sdk": "^2.291.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "cobertura",
      "html"
    ],
    "all": true
  }
}