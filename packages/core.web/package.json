{
  "name": "@applicature/synth.web",
  "version": "2.0.0",
  "private": false,
  "license": "UNLICENSED",
  "description": "Web Plugin",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "build": "gulp clean && gulp build",
    "lint": "tslint '{index.ts,src/**/*.ts,test/**/*.ts}'",
    "precommit": "npm run lint ; npm run test:ci",
    "test": "jest",
    "test:ci": "jest --coverage --forceExit",
    "test:sonar": "sonar-scanner -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$SONARCLOUD_TOKEN",
    "test:lint": "tslint --force --out ./test-reports/tslint.json -t json --project ./tsconfig.json '{index.ts,src/**/*.ts,test/**/*.ts}'"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:applicature/multivest.web.git"
  },
  "dependencies": {
    "@applicature/synth.plugin-manager": "^2.0.0",
    "@types/morgan": "^1.7.35",
    "compression": "^1.7.1",
    "config": "^1.29.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.9.0",
    "hoek": "5.0.3",
    "http-status": "^1.0.1",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jest-multi-test-result-processor": "^1.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "joi": "13.1.1",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "raven": "^2.4.2",
    "sonarqube-scanner": "^2.1.0",
    "swagger-stats": "^0.95.6",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "@types/compression": "0.0.35",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.0",
    "@types/helmet": "0.0.37",
    "@types/joi": "^13.0.5",
    "@types/method-override": "0.0.31",
    "@types/passport": "^0.4.6",
    "@types/raven": "^2.1.6",

    "@types/config": "0.0.34",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-cli": "^20.0.4",
    "jest-junit": "^5.2.0",
    "jest-multi-test-result-processor": "^1.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.1.1",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "jest-multi-test-result-processor": {
    "processors": [
      "jest-junit",
      "jest-sonar-reporter"
    ]
  },
  "jestSonar": {
    "reportPath": "./test-reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./test-reports/junit.xml"
  }
}
