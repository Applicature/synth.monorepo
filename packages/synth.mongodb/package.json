{
  "name": "@applicature/synth.mongodb",
  "version": "2.0.16",
  "private": false,
  "license": "Apache-2.0",
  "description": "MongoDB Plugin",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp clean && gulp build",
    "lint": "tslint '{index.ts,src/**/*.ts,test/**/*.ts}'",
    "test": "jest",
    "test:ci": "jest --coverage --forceExit",
    "test:sonar": "sonar-scanner -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$SONARCLOUD_TOKEN",
    "test:lint": "tslint --force --out ./test-reports/tslint.json -t json --project ./tsconfig.json '{index.ts,src/**/*.ts,test/**/*.ts}'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Applicature/synth.monorepo.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@applicature/synth.plugin-manager": "^2.0.16",
    "bignumber.js": "^7.2.1",
    "config": "^1.30.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.8",
    "uuid": "^3.2.1",
    "winston": "^2.4.1"
  },
  "files": [
    "dist/",
    "src/",
    "index.d.ts",
    "index.js"
  ],
  "devDependencies": {
    "@types/agenda": "^1.0.2",
    "@types/config": "0.0.33",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.116",
    "@types/mongodb": "^3.1.12",
    "@types/node": "^8.10.1",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.8",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^3.2.4",
    "husky": "^0.14.3",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jest": "^23.4.2",
    "jest-cli": "^22.4.3",
    "jest-junit": "^3.6.0",
    "jest-multi-test-result-processor": "^1.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "mongo-mock": "^3.1.0",
    "nsp": "^3.2.1",
    "sonarqube-scanner": "^2.1.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "jest-multi-test-result-processor": {
    "processors": [
      "jest-junit",
      "jest-sonar-reporter"
    ]
  },
  "jestSonar": {
    "reportPath": "./test-reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./test-reports/junit.xml"
  },
  "gitHead": "5bd4034fc2f3c25d8e116a2c611f1ce96ec40f7b"
}
